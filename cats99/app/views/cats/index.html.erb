<h1>All the Cats!</h1>

<ul> 
    <% @cats.each do |cat| %>
        <h3>Name: <%= cat.name%> </h3>
        <li>Birthday: <%= cat.birth_date %></li>
        <li>Age: <%=  cat.age %> </li>
        <li>Color: <%= cat.color %> </li>
        <li>Sex: <%=  cat.sex %> </li>
        <li>Description: <%= cat.description%>
        
        <li> <a href="<%= cat_url(cat)%>"> <%= cat.name %>'s Individual page</a> </li>
        <br>
    <% end %>
</ul>